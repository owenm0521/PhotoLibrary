<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.view.AdminLoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="usersListView" layoutX="11.0" layoutY="42.0" prefHeight="286.0" prefWidth="350.0" />
            <Label layoutX="457.0" layoutY="94.0" prefHeight="45.0" prefWidth="115.0" text="Create New User">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
            <Label layoutX="377.0" layoutY="153.0" prefHeight="45.0" prefWidth="67.0" text="Username">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
            <TextField fx:id="newUsername" layoutX="457.0" layoutY="162.0" prefHeight="27.0" prefWidth="127.0" />
            <Button onAction="#deleteUser" layoutX="146.0" layoutY="348.0" mnemonicParsing="false" text="Delete User">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font></Button>
            <Label layoutX="157.0" layoutY="14.0" prefHeight="27.0" prefWidth="61.0" text="User List">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
            <Button onAction="#createNewUser" layoutX="459.0" layoutY="210.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="127.0" text="Create New User">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font></Button>
            <Button onAction="#logOut" layoutX="566.0" layoutY="15.0" mnemonicParsing="false" text="Log Out">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Button>
         </children>
    </AnchorPane>
  </children>
</VBox>
