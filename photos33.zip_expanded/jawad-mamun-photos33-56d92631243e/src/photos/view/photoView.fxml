<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="782.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photos.view.PhotoViewController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="543.0" prefWidth="782.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="albumName" layoutX="27.0" layoutY="25.0" prefHeight="17.0" prefWidth="400.0" text="Album Name:">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
            <ListView fx:id="photosListView" layoutX="23.0" layoutY="61.0" prefHeight="363.0" prefWidth="468.0" />
            <Button layoutX="205.0" layoutY="434.0" mnemonicParsing="false" onAction="#addPicture" prefHeight="37.0" prefWidth="130.0" text="Add Picture">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Button>
            <ImageView fx:id="selectedPhotoImageView" fitHeight="150.0" fitWidth="265.0" layoutX="508.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
            <TextField fx:id="displayedPhotoDate" layoutX="567.0" layoutY="216.0" prefHeight="27.0" prefWidth="200.0" />
            <Label layoutX="508.0" layoutY="221.0" prefHeight="17.0" prefWidth="59.0" text="Date:">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
            <Label layoutX="508.0" layoutY="251.0" prefHeight="17.0" prefWidth="59.0" text="Caption:">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
            <TextField fx:id="displayedPhotoCaption" layoutX="567.0" layoutY="246.0" prefHeight="27.0" prefWidth="130.0" />
            <Button layoutX="502.0" layoutY="174.0" mnemonicParsing="false" onAction="#deletePicture" prefHeight="27.0" prefWidth="114.0" text="Delete Picture">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Button>
            <Button layoutX="635.0" layoutY="174.0" mnemonicParsing="false" onAction="#moveOrCopyPicture" prefHeight="27.0" prefWidth="141.0" text="Move/Copy Picture">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Button>
            <ListView fx:id="selectedPhotoTags" layoutX="507.0" layoutY="307.0" prefHeight="127.0" prefWidth="248.0" />
            <TextField fx:id="inputedTagValue" layoutX="641.0" layoutY="445.0" prefHeight="27.0" prefWidth="114.0" promptText="Value" />
            <Button layoutX="698.0" layoutY="246.0" mnemonicParsing="false" onAction="#confirmCaption" prefHeight="9.0" prefWidth="73.0" text="Confirm">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Button>
            <Button layoutX="510.0" layoutY="485.0" mnemonicParsing="false" onAction="#addTag" prefHeight="27.0" prefWidth="114.0" text="Add ">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Button>
            <Button layoutX="640.0" layoutY="485.0" mnemonicParsing="false" onAction="#deleteTag" prefHeight="27.0" prefWidth="114.0" text="Delete ">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Button>
            <Button layoutX="14.0" layoutY="547.0" mnemonicParsing="false" onAction="#returnToPreviousPage" prefHeight="27.0" prefWidth="114.0" text="Back">
   				<font>
      				<Font name="American Typewriter" size="13.0" />
   				</font>
			</Button>
            <ComboBox fx:id="inputedTagName" editable="true" layoutX="510.0" layoutY="445.0" prefHeight="27.0" prefWidth="114.0" promptText="Tag" />
            <Label layoutX="595.0" layoutY="283.0" prefHeight="17.0" prefWidth="80.0" text="Photo Tags:">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
            <Label layoutX="502.0" layoutY="534.0" prefHeight="27.0" prefWidth="259.0" text="To add/delete tags, insert tag information">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
            <Label layoutX="530.0" layoutY="561.0" prefHeight="27.0" prefWidth="211.0" text="and click the appropriate button.">
               <font>
                  <Font name="American Typewriter" size="13.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
